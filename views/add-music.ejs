<%- include('dashboard/imports/header'); -%>
<main id="dashboard" class="flx">
  <%- include('dashboard/imports/nav'); -%>
  <div class="content">
    <section id="sec-1">
      <form action="" class="add-music-form">
        <h3 class="f-size-xlrg">Add Song</h3>
        <div class="form-input grd grd-cl-4">
          <div class="input">
            <label for="title">Title</label>
            <input type="text" name="title" id="" />
            <p class="error title-error"></p>
          </div>
          <div class="input">
            <label for="artist">Artist</label>
            <input type="text" name="artist" id="" />
            <p class="error artist-error"></p>
          </div>
          <div class="input">
            <label for="album">Album</label>
            <input type="text" name="album" id="" />
            <p class="error album-error"></p>
          </div>
          <div class="input">
            <label for="trackLength">Track Length</label>
            <input type="text" name="trackLength" id="" />
            <p class="error trackLength-error"></p>
          </div>
          <div class="input">
            <label for="musicFile">Music File</label>
            <input type="file" name="musicFile" id="" />
            <p class="error musicFile-error"></p>
          </div>
        </div>
        <div class="input">
          <button type="submit" class="btn">Add</button>
        </div>
      </form>
    </section>
  </div>
</main>
<%- include('dashboard/imports/footer-nav'); -%>
<script src="/js/dashboard.addmusic.js"></script>
<%- include('dashboard/imports/footer'); -%>
